{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "routes": [
      { "src": "/api/og", "dest": "/api/og" },
      { "src": "/assets/(.*)", "dest": "/build/client/assets/$1" },
      { "src": "/build/client/(.*)", "dest": "/build/client/$1" },
      { "src": "/(.*)", "dest": "/api/index" }
    ]
  }